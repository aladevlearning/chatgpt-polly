{
  "version": 1,
  "paths": {
    "/ask": {
      "name": "/ask",
      "lambdaFunction": "askByVoice",
      "permissions": {
        "setting": "private",
        "auth": [
          "create",
          "read",
          "update",
          "delete"
        ]
      }
    },
    "/askWithDocument": {
      "name": "/askWithDocument",
      "lambdaFunction": "amplifyLangChain",
